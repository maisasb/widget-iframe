,MAAM,CAACkB,aAAP,CAAqB+C,MAAlC,EAA0C;AAC/ChD,IAAAA,IAAI,GAAG;AACLD,MAAAA,IAAI,EAAEA,IADD;AAELY,MAAAA,MAAM,EAAE,CAFH;AAGLR,MAAAA,EAAE,EAAE2D,YAHC;AAILxE,MAAAA,QAAQ,oBAAOuE,gBAAP,CAJH;AAKLE,MAAAA,gBAAgB,EAAE,IALb;AAMLC,MAAAA,iBAAiB,EAAE,IANd;AAOLC,MAAAA,eAAe,EAAE,IAPZ;AAQLC,MAAAA,aAAa,EAAEJ,YARV;AASLjD,MAAAA,KAAK,EAAE,EATF;AAULC,MAAAA,kBAAkB,EAAE,KAVf;AAWLC,MAAAA,UAAU,EAAE;AACVC,QAAAA,SAAS,EAAE;AACTC,UAAAA,MAAM,EAAElC,MAAM,CAACmC,YAAP,CAAoBC,aADnB;AAETC,UAAAA,KAAK,EAAE;AAFE,SADD;AAKVC,QAAAA,OAAO,EAAE;AACPJ,UAAAA,MAAM,EAAElC,MAAM,CAACmC,YAAP,CAAoBC,aADrB;AAEPC,UAAAA,KAAK,EAAE;AAFA,SALC;AASVE,QAAAA,QAAQ,EAAE;AACRL,UAAAA,MAAM,EAAElC,MAAM,CAACmC,YAAP,CAAoBC,aADpB;AAERC,UAAAA,KAAK,EAAE;AAFC;AATA;AAXP,KAAP;AA0BD;;AACD,SAAOpB,IAAP;AACD","sourcesContent":["import { consts } from '../../boot/consts'\nimport store from '../../utils/store'\nimport SurveyUseCase from './usecase'\n\nconst commentDefault = {\n  'pt_BR': 'O que motivou sua opinião',\n  'en_US': 'What motivated your opinion',\n  'es_ES': 'Lo que motivó su opinión'\n}\nconst questionCes = {\n  'pt_BR': 'Até que ponto você concorda com a seguinte declaração: A empresa facilitou o meu tratamento do problema.',\n  'en_US': 'To what extent do you agree with the following statement: The company made it easy for me to handle my issue.',\n  'es_ES': '¿Hasta qué punto está de acuerdo con la siguiente declaración: La compañía ha facilitado mi tratamiento del problema.'\n}\nconst questionCsat = {\n  'pt_BR': 'O quanto você ficou satisfeito com o produto/serviço?',\n  'en_US': 'How satisfied were you with the product / service?',\n  'es_ES': '¿Qué tan satisfecho estuvo con el producto / servicio?'\n}\nconst questionNpsTransactional = {\n  'pt_BR': `Em uma escala de 0 a 10, o quanto você recomendaria o serviço de suporte da empresa para um amigo ou familiar?`,\n  'en_US': `On a scale of 0 to 10, how much would you recommend the company's support or services to a friend or family member?`,\n  'es_ES': `En una escala de 0 a 10, ¿cuánto recomendaría el servicio de soporte de la compañía a un amigo o familiar?`\n}\nconst question = {\n  'pt_BR': 'Titulo da questão',\n  'en_US': 'Question title',\n  'es_ES': 'Titulo da questão'\n}\nconst buttonSubtitleCsatMin = {\n  'pt_BR': 'Pouco Satisfeito',\n  'en_US': 'Not Satisfied',\n  'es_ES': 'Poco Satisfecho'\n}\nconst buttonSubtitleCsatMax = {\n  'pt_BR': 'Muito Satisfeito',\n  'en_US': 'Very Satisfied',\n  'es_ES': 'Muy Satisfecho'\n}\nconst buttonSubtitleCesMin = {\n  'pt_BR': 'Discordo Totalmente',\n  'en_US': 'Strongly Disagree',\n  'es_ES': 'Totalmente en desacuerdo'\n}\nconst buttonSubtitleCesMax = {\n  'pt_BR': 'Concordo Totalmente',\n  'en_US': 'Strongly Agree',\n  'es_ES': 'Totalmente de acuerdo'\n}\n\nconst questionNPS = (organizationName) => {\n  return {\n    'pt_BR': `Em uma escala de 0 a 10, o quanto você recomendaria a ${organizationName} para um amigo ou famíliar?`,\n    'en_US': `On a scale from 0 to 10, how likely are you to recommend ${organizationName} to a friend or colleague?`,\n    'es_ES': `En una escala de 0 a 10, ¿cuánto recomendaría ${organizationName} a un amigo o familiar?`\n  }\n}\n\nexport const DEFAULT_COMPOSITE_QUESTION = {\n  'pt_BR': 'Deixe sua opinião',\n  'en_US': 'Leave your opinion',\n  'es_ES': 'Deja tu opinion'\n}\n\nexport function createSurveyItem (type) {\n  let item = null\n  if (type === consts.QUESTION_TYPE.LABEL) {\n    item = {\n      type: type,\n      id: new Date().getTime(),\n      question: {\n        'pt_BR': 'Digite um rótulo',\n        'en_US': 'Type a label',\n        'es_ES': 'Escribe una etiqueta'\n      }\n    }\n  } else if (type === consts.QUESTION_TYPE.IMAGE) {\n    item = {\n      type: type,\n      id: new Date().getTime(),\n      url: '',\n      extension: '',\n      align: 'center'\n\n    }\n  } else if (type === consts.QUESTION_TYPE.OPEN) {\n    item = {\n      type: type,\n      weight: 0,\n      id: new Date().getTime(),\n      question: { ...question },\n      composite_question: { ...DEFAULT_COMPOSITE_QUESTION },\n      items: [],\n      jump_rules_enabled: false,\n      jump_rules: {\n        detractor: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        passive: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        promoter: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        }\n      }\n    }\n  } else if (type === consts.QUESTION_TYPE.RATINGS) {\n    item = {\n      type: type,\n      weight: 0,\n      id: new Date().getTime(),\n      question: { ...question },\n      comment_question: { ...commentDefault },\n      min_scale: 1,\n      max_scale: 5,\n      button_design: 1,\n      color_pattern: 1,\n      custom_color: '#aaabb3',\n      composite_question: { ...DEFAULT_COMPOSITE_QUESTION },\n      comments_enabled: true,\n      items: [],\n      jump_rules_enabled: false,\n      jump_rules: {\n        detractor: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        passive: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        promoter: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        }\n      }\n    }\n  } else if (type === consts.QUESTION_TYPE.CES2) {\n    const surveyUseCase = new SurveyUseCase()\n    item = {\n      type: type,\n      weight: 0,\n      id: new Date().getTime(),\n      question: { ...surveyUseCase.getCes2Question() },\n      comment_question: { ...commentDefault },\n      scale_type: 0,\n      min_scale: 1,\n      max_scale: 5,\n      button_design: 1,\n      button_subtitle_min: { ...surveyUseCase.getCes2MinButtonScale(5) },\n      button_subtitle_max: { ...surveyUseCase.getCes2MaxButtonScale(5) },\n      color_pattern: 1,\n      custom_color: '#aaabb3',\n      composite_question: { ...DEFAULT_COMPOSITE_QUESTION },\n      comments_enabled: true,\n      items: [],\n      jump_rules_enabled: false,\n      jump_rules: {\n        detractor: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        passive: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        promoter: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        }\n      }\n    }\n  } else if (type === consts.QUESTION_TYPE.NPS_RELATIONAL || type === consts.QUESTION_TYPE.NPS_TRANSACTIONAL) {\n    item = {\n      type: type,\n      weight: 0,\n      id: new Date().getTime(),\n      question: type === consts.QUESTION_TYPE.NPS_TRANSACTIONAL ? { ...questionNpsTransactional } : { ...questionNPS(store.getState().organization.organization.name) },\n      detractor_comment_question: { ...commentDefault },\n      neutral_comment_question: { ...commentDefault },\n      promoter_comment_question: { ...commentDefault },\n      button_design: 1,\n      color_pattern: 1,\n      inverted_scale: false,\n      min_scale: 0,\n      max_scale: 10,\n      custom_color: '#aaabb3',\n      composite_question: { ...DEFAULT_COMPOSITE_QUESTION },\n      comments_enabled: true,\n      items: [],\n      jump_rules_enabled: false,\n      jump_rules: {\n        detractor: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        passive: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        promoter: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        }\n      }\n    }\n  } else if (type === consts.QUESTION_TYPE.THUMBS) {\n    item = {\n      type: type,\n      weight: 0,\n      id: new Date().getTime(),\n      question: { ...question },\n      detractor_comment_question: { ...commentDefault },\n      promoter_comment_question: { ...commentDefault },\n      composite_question: { ...DEFAULT_COMPOSITE_QUESTION },\n      comments_enabled: true,\n      items: [],\n      jump_rules_enabled: false,\n      jump_rules: {\n        detractor: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        passive: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        promoter: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        }\n      }\n    }\n  } else if (type === consts.QUESTION_TYPE.CES || type === consts.QUESTION_TYPE.CSAT) {\n    let subtitleMin = {}\n    let subtitleMax = {}\n    let questionTitle = {}\n\n    if (type === consts.QUESTION_TYPE.CES) {\n      subtitleMin = buttonSubtitleCesMin\n      subtitleMax = buttonSubtitleCesMax\n      questionTitle = { ...questionCes }\n    } else {\n      subtitleMin = buttonSubtitleCsatMin\n      subtitleMax = buttonSubtitleCsatMax\n      questionTitle = { ...questionCsat }\n    }\n\n    item = {\n      type: type,\n      weight: 0,\n      id: new Date().getTime(),\n      question: questionTitle,\n      comment_question: { ...commentDefault },\n      button_design: 1,\n      color_pattern: 1,\n      min_scale: 1,\n      max_scale: 5,\n      scale_type: 0,\n      button_subtitle_min: { ...subtitleMin },\n      button_subtitle_max: { ...subtitleMax },\n      custom_color: '#aaabb3',\n      composite_question: { ...DEFAULT_COMPOSITE_QUESTION },\n      comments_enabled: true,\n      items: [],\n      jump_rules_enabled: false,\n      jump_rules: {\n        detractor: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        passive: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        promoter: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        }\n      }\n    }\n  } else if (type === consts.QUESTION_TYPE.EMOJI) {\n    item = {\n      type: type,\n      weight: 0,\n      id: new Date().getTime(),\n      question: { ...question },\n      comment_question: { ...commentDefault },\n      button_design: 3,\n      color_pattern: 1,\n      min_scale: 1,\n      max_scale: 5,\n      custom_color: '#aaabb3',\n      composite_question: { ...DEFAULT_COMPOSITE_QUESTION },\n      comments_enabled: true,\n      items: [],\n      jump_rules_enabled: false,\n      jump_rules: {\n        detractor: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        passive: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        promoter: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        }\n      }\n    }\n  } else {\n    item = {\n      type: type,\n      weight: 0,\n      id: new Date().getTime(),\n      question: { ...question },\n      comment_question: { ...commentDefault },\n      horizontal_align: false,\n      multiple_answers: false,\n      option_other: false,\n      randomize: false,\n      options: [\n        { 'pt_BR': 'Insira uma resposta', 'en_US': 'Insert a answer', 'es_ES': 'Ingresa una respuesta' }\n      ],\n      composite_question: { ...DEFAULT_COMPOSITE_QUESTION },\n      comments_enabled: true,\n      items: [],\n      jump_rules_enabled: false,\n      jump_rules: {\n        detractor: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        passive: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        promoter: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        }\n      }\n    }\n  }\n\n  return item\n}\n\nexport function createCompositeMetricsItem (type, categoryQuestion, categoryUUID) {\n  let item = null\n  if (type === consts.QUESTION_TYPE.LABEL) {\n    item = {\n      type: type,\n      id: categoryUUID,\n      question: { ...categoryQuestion },\n      items: []\n    }\n  } else if (type === consts.QUESTION_TYPE.THUMBS) {\n    item = {\n      type: type,\n      weight: 0,\n      id: categoryUUID,\n      question: { ...categoryQuestion },\n      promoter_enabled: true,\n      detractor_enabled: true,\n      neutral_enabled: true,\n      category_uuid: categoryUUID,\n      items: [],\n      jump_rules_enabled: false,\n      jump_rules: {\n        detractor: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        passive: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        },\n        promoter: {\n          action: consts.JUMP_ACTIONS.NEXT_QUESTION,\n          value: null\n        }\n      }\n    }\n  }\n  return item\n}\n"]}]}