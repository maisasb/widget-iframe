D ? 'widget-embebed' : 'widget-fixed'\"\n    class=\"box\"\n    :style=\"{background: colors.secondaryColor}\">\n    <div id=\"track__widget-close\" class=\"icon-close\">\n      <Icon name=\"Close\" :color=\"colors.primaryColor\" class=\"cursor-pointer icon-close\" />\n    </div>\n    <div class=\"track-question\">\n      Teste widget\n    </div>\n    <div class=\"track-buttons\">\n      <a href=\"https://track.co\" target=\"_blank\"><img :src=\"logo\" width=\"30\"></a>\n      <Button :label=\"userLanguage.survey.buttons.next\" />\n      <Button :label=\"userLanguage.survey.buttons.send\" />\n      <Button :label=\"userLanguage.survey.buttons.previous\" />\n    </div>\n  </div>\n</template>\n<script>\nimport consts from '../../../consts'\nimport Icon from '../../../components/Icon'\nimport { Button } from '@tracksale/design-system'\nimport { mapState } from 'vuex'\nimport { userLanguage } from '../../../utils/language'\n\nexport default {\n  components: { Icon, Button },\n  props: {\n    widget: Object\n  },\n  data () {\n    return {\n      consts: consts,\n      logo: 'data:image/png;base64,'\n    }\n  },\n  computed: {\n    ...mapState({\n      language: state => state.survey.language\n    }),\n    colors () {\n      let primaryColor = this.widget.survey.primary_color || '#354052'\n      let secondaryColor = '#fff'\n      if (!this.widget.survey_colors) {\n        secondaryColor = this.widget.secondary_color\n        primaryColor = this.widget.primary_color\n      }\n      return { primaryColor, secondaryColor }\n    }\n  }\n\n\n}\n</script>\n<style scoped>\n.box {\n  background-color: white;\n  border-top-left-radius: 12px;\n  border-top-right-radius: 12px;\n  border: 1px solid transparent;\n  box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.25);\n}\n.widget-fixed {\n  position: fixed;\n  bottom: 20px;\n  max-height: 600px;\n  max-width: 450px;\n  width: 100%;\n  right: 0;\n  z-index: 99999;\n}\n.widget-embebed {\n  position: relative;\n  width: 100%;\n  max-height: 600px;\n}\n.icon-close {\n  padding: 10px;\n  text-align: right;\n}\n.cursor-pointer {\n  cursor: pointer;\n}\n.track-buttons {\n  margin-bottom: 20px;\n  padding-left: 20px;\n  padding-right: 20px;\n}\n.track-question {\n  max-height: 500px;\n  overflow: auto;\n  padding: 20px;\n}\n</style>"],"sourceRoot":"src/views/Widget/components"}]}